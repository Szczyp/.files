set hintmatching=wordstartswith,transliterated

set go=s
set defsearch=duckduckgo

nmap -b d <c-d>
nmap -b u <c-u>
nmap -b <c-d> <Nop>
nmap -b <c-u> <Nop>
nmap -b x D
nmap -b X d
nmap -b U u
nmap -b j 3j
nmap -b k 3k
nmap -b h 3h
nmap -b l 3l
nmap -b J <c-p>
nmap -b K <c-n>
nmap -b <c-p> <Nop>
nmap -b <c-n> <Nop>
