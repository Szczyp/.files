set hintmatching=wordstartswith,transliterated
set hintchars="HJKLFDSGUIOPREWNMVC"

set defsearch=duckduckgo

set gui scrollbars=false
set gui complete=sl
set gui animations=true

colorscheme sweets_snaka

nnoremap d <c-d>
nnoremap u <c-u>
nnoremap <c-d> <Nop>
nnoremap <c-u> <Nop>
nnoremap x D
nnoremap X d
nnoremap U u
nnoremap j 3j
nnoremap k 3k
nnoremap h 3h
nnoremap l 3l
nnoremap K <c-p>
nnoremap J <c-n>
nnoremap <c-p> <Nop>
nnoremap <c-n> <Nop>
nnoremap s :panorama switch 
